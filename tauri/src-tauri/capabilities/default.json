{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "shell:default",
    "process:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "sidecar": true,
          "name": "bin/nodejs",
          "args": [
            "/S*"
          ]
        }
      ]
    },
    "fs:default"
  ]
}